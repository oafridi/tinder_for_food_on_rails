<div class="center jumbotron">
  <h3> Click like or dislike! </h3>
  <p>
    <%= image_tag(@item.pic_url, size: "500x500", :alt => "rss feed") %>
  </p>
  <%= link_to "dislike", {:controller => :pages, :action => :dislike, :params => {:user_id => @user.id, :item_id => @item.id} }, class: "btn btn-lg btn-primary" %>
  <%= link_to "like", {:controller => :pages, :action => :like, :params => {:user_id => @user.id, :item_id => @item.id} }, class: "btn btn-lg btn-primary" %>
</div>